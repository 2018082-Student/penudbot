<?xml version="1.0" encoding="utf-8"?>
<robot
  name="pendubot">
  <link
    name="base_link">
    <inertial>
      <origin xyz="0 0 3" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="0"  ixy="0"  ixz="0" iyy="0" iyz="0" izz="0" />
    </inertial>
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.1"/>
      </geometry>
    </visual>
  </link>
  
  <link
    name="link_1">
    <inertial>
      <mass value="1"/>
      <origin xyz = "0 0 0.25" />
      <inertia ixx="0.020833"  ixy="0"  ixz="0" iyy="0.020833" iyz="0" izz="0.02083333" />
    </inertial>
    <visual>
			<origin xyz = "0 0 0.25" />
			<geometry>
				<cylinder radius = "0.05" length = "0.5"  />
			</geometry>
			<material name = "gray 1">
				<color rgba = "0.2 0.9 0.3 1" />
			</material>
		</visual>
  </link>
  <link
    name="link_2">
    <inertial>
      <mass value="1"/>
      <origin xyz = "0 0 0.25" />
      <inertia ixx="0.020833"  ixy="0"  ixz="0" iyy="0.020833" iyz="0" izz="0.02083333" />
    </inertial>
    <visual>
			<origin xyz = "0 0 0.25" />
			<geometry>
				<cylinder radius = "0.05" length = "0.5"  />
			</geometry>
			<material name = "gray 1">
				<color rgba = "0.5 0.9 0.3 1" />
			</material>
		</visual>
  </link>

  <joint name = "joint_1" type = "revolute">
		<parent link = "base_link" />
		<child link = "link_1" />
		<origin xyz = "0 0 0" />
		<axis xyz = "1 0 0" />
    <limit
      lower="-1000.0"
      upper="1000.0" effort="1000.0" velocity="1000.0"/>
    <dynamics damping="0.1"/>
	</joint>

  <joint name = "joint_2" type = "revolute">
		<parent link = "link_1" />
		<child link = "link_2" />
		<origin xyz = "0 0 0.5" />
		<axis xyz = "1 0 0" />
    <limit
      lower="-1000.0"
      upper="1000.0" effort="1000.0" velocity="1000.0"/>
    <dynamics damping="0.1"/>
	</joint>



</robot>